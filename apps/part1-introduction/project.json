{
  "name": "part1-introduction",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/part1-introduction/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/part1-introduction/**/*.{ts,svelte,spec.ts}"
        ]
      }
    },
    "check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "svelte-check",
        "cwd": "apps/part1-introduction"
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/part1-introduction"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "part1-introduction:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "part1-introduction:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "part1-introduction:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "part1-introduction:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "part1-introduction:build:development"
        },
        "production": {
          "buildTarget": "part1-introduction:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/part1-introduction"
      }
    }
  }
}
